    "eslint-plugin-prettier": "^4.2.1",
    "firebase": "^9.17.1",
    "firebase-admin": "^11.5.0",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "typescript": "^4.1.5",
    "vitest": "^0.28.4"
